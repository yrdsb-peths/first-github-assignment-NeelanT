public class ArrayTester extends ConsoleProgram
{
    public void run()
    {
        ExpandingArray arr = new ExpandingArray();
        
        System.out.println("adding ");
        arr.add(1, 5);
    
        for(int i = 0; i < 100; i++)
        {
            System.out.println(arr.get(i));
        }
        
    }
}
